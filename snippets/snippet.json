{
  "CREATE_JENKINS_BATCH": {
    "prefix": "create_jenkins_batch",
    "body": [
      "#!jenkins",
      "",
      "create-user     dev1 dev1234                # create user (account: dev1, password: dev1234)",
      "create-user     dev2 dev1234                # create user",
      "create-views    demo-view1 demo-view2       # create demo-view1, demo-view2 with Default Regex [a-zA-z].*",
      "create-view     link-view1 .*-link          # create View with regex \".*-link\"",
      "create-folder   my-folder                   # create folder",
      "",
      "create-pipeline dev-web",
      "create-shortcut home-link  https://jenkinssuite.github.io",
      "",
      "# create-global-var  GIT_SERVER http://192.168.0.1",
      "# create-global-var  env        dev",
      "# create-cred-User   user-git   git1234     # http://{JENKINS_SERVER}/jenkins/manage/credentials/",
      "# create-secret-text user-git   git1234     # http://{JENKINS_SERVER}/jenkins/manage/credentials/",
      "",
      "# delete-user dev2",
      "# delete-view demo-view2",
      "# delete-job  job/dev-web",
      ""
    ],
    "description": "Create Jenkins Batch Script",
    "language": "jkssh",
    "type": "system"
  },
  "C_DSL_VIEW_ALL": {
    "prefix": "c_dsl_view_all",
    "body": [
      "<?xml version=\"1.1\" encoding=\"UTF-8\"?><project>",
      "  <actions/>",
      "  <description/>",
      "  <keepDependencies>false</keepDependencies>",
      "  <properties>",
      "    <com.sonyericsson.rebuild.RebuildSettings plugin=\"rebuild@1.33.1\">",
      "      <autoRebuild>false</autoRebuild>",
      "      <rebuildDisabled>false</rebuildDisabled>",
      "    </com.sonyericsson.rebuild.RebuildSettings>",
      "    <hudson.model.ParametersDefinitionProperty>",
      "      <parameterDefinitions>",
      "        <hudson.model.ChoiceParameterDefinition>",
      "          <name>view</name>",
      "          <description>Create a Tab view page.",
      "",
      "all: __All__",
      "dev: 10_Dev, 30_Stg",
      "prd: 10_Prd</description>",
      "          <choices class=\"java.util.Arrays$ArrayList\">",
      "            <a class=\"string-array\">",
      "              <string/>",
      "              <string>all</string>",
      "              <string>dev</string>",
      "              <string>prd</string>",
      "            </a>",
      "          </choices>",
      "        </hudson.model.ChoiceParameterDefinition>",
      "      </parameterDefinitions>",
      "    </hudson.model.ParametersDefinitionProperty>",
      "  </properties>",
      "  <scm class=\"hudson.scm.NullSCM\"/>",
      "  <canRoam>true</canRoam>",
      "  <disabled>false</disabled>",
      "  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>",
      "  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>",
      "  <triggers/>",
      "  <concurrentBuild>false</concurrentBuild>",
      "  <builders>",
      "    <javaposse.jobdsl.plugin.ExecuteDslScripts plugin=\"job-dsl@1.79\">",
      "      <scriptText>listView(&apos;ZZ_templates&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/_.*/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "",
      "listView(&apos;_All_&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "",
      "if (view == &apos;dev&apos;) {",
      "listView(&apos;10_Dev&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_dev/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "listView(&apos;30_STG&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_stg/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "}",
      "",
      "if (view == &apos;prd&apos;) {",
      "listView(&apos;10_Prd&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_prd/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "}</scriptText>",
      "      <usingScriptText>true</usingScriptText>",
      "      <sandbox>false</sandbox>",
      "      <ignoreExisting>false</ignoreExisting>",
      "      <ignoreMissingFiles>false</ignoreMissingFiles>",
      "      <failOnMissingPlugin>false</failOnMissingPlugin>",
      "      <failOnSeedCollision>false</failOnSeedCollision>",
      "      <unstableOnDeprecation>false</unstableOnDeprecation>",
      "      <removedJobAction>IGNORE</removedJobAction>",
      "      <removedViewAction>IGNORE</removedViewAction>",
      "      <removedConfigFilesAction>IGNORE</removedConfigFilesAction>",
      "      <lookupStrategy>SEED_JOB</lookupStrategy>",
      "    </javaposse.jobdsl.plugin.ExecuteDslScripts>",
      "  </builders>",
      "  <publishers/>",
      "  <buildWrappers/>",
      "</project>"
    ],
    "description": "Create a tabbed view page",
    "when": "plugin.jobdsl.enabled",
    "type": "system"
  },
  "C_DSL_JOB": {
    "prefix": "c_dsl_job",
    "body": [
      "<?xml version='1.1' encoding='UTF-8'?>",
      "<project>",
      "  <actions/>",
      "  <description>Create a job for server configuration.&#xd;",
      "&#xd;",
      "    - __PROJECT__-_dev</description>",
      "  <keepDependencies>false</keepDependencies>",
      "  <properties>",
      "    <com.sonyericsson.rebuild.RebuildSettings plugin=\"rebuild@320.v5a_0933a_e7d61\">",
      "      <autoRebuild>false</autoRebuild>",
      "      <rebuildDisabled>false</rebuildDisabled>",
      "    </com.sonyericsson.rebuild.RebuildSettings>",
      "    <hudson.model.ParametersDefinitionProperty>",
      "      <parameterDefinitions>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>PID</name>",
      "          <description>Enter your project code</description>",
      "          <defaultValue>PID</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>PROJ_NAME</name>",
      "          <description>Enter the name of your project</description>",
      "          <defaultValue>web</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>ENV</name>",
      "          <description>Enter your deployment environment [dev, stg, prd]</description>",
      "          <defaultValue>dev</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>CREDENTIALS</name>",
      "          <description>credentials of the Git </description>",
      "          <defaultValue>PID-git</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "      </parameterDefinitions>",
      "    </hudson.model.ParametersDefinitionProperty>",
      "  </properties>",
      "  <scm class=\"hudson.scm.NullSCM\"/>",
      "  <canRoam>true</canRoam>",
      "  <disabled>false</disabled>",
      "  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>",
      "  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>",
      "  <triggers/>",
      "  <concurrentBuild>false</concurrentBuild>",
      "  <builders>",
      "    <javaposse.jobdsl.plugin.ExecuteDslScripts plugin=\"job-dsl@1.83\">",
      "      <scriptText>pipelineJob(&quot;${PID}-${PROJ_NAME}_${ENV}&quot;) {",
      "    description(&quot;${PID}-${PROJ_NAME}_${ENV}&quot;)",
      "    definition {",
      "        cpsScm {",
      "            scm {",
      "                git {",
      "                  remote {",
      "                    name(&apos;origin&apos;)",
      "                    url(&quot;http://$GIT_SERVER/${PID}-${PROJ_NAME}.git&quot;)",
      "                    credentials(&quot;$CREDENTIALS&quot;)",
      "                  }",
      "                  branch(&apos;*/${ENV}&apos;)",
      "                  scriptPath(&quot;Jenkinsfile_dev.groovy&quot;)",
      "                }",
      "            }",
      "            lightweight()",
      "        }",
      "    }",
      "}</scriptText>",
      "      <usingScriptText>true</usingScriptText>",
      "      <sandbox>false</sandbox>",
      "      <ignoreExisting>false</ignoreExisting>",
      "      <ignoreMissingFiles>false</ignoreMissingFiles>",
      "      <failOnMissingPlugin>false</failOnMissingPlugin>",
      "      <failOnSeedCollision>false</failOnSeedCollision>",
      "      <unstableOnDeprecation>false</unstableOnDeprecation>",
      "      <removedJobAction>IGNORE</removedJobAction>",
      "      <removedViewAction>IGNORE</removedViewAction>",
      "      <removedConfigFilesAction>IGNORE</removedConfigFilesAction>",
      "      <lookupStrategy>JENKINS_ROOT</lookupStrategy>",
      "    </javaposse.jobdsl.plugin.ExecuteDslScripts>",
      "  </builders>",
      "  <publishers/>",
      "  <buildWrappers/>",
      "</project>"
    ],
    "description": "Create a pipeline job",
    "when": "plugin.jobdsl.enabled",
    "type": "system"
  },
  "C_JOB_PIPELINE_SCM": {
    "prefix": "c_job_pipeline_scm",
    "body": [
      "<?xml version='1.1' encoding='UTF-8'?>",
      "<flow-definition plugin=\"workflow-job@1316.vd2290d3341a_f\">",
      "  <actions>",
      "    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobAction plugin=\"pipeline-model-definition@2.2141.v5402e818a_779\"/>",
      "    <org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction plugin=\"pipeline-model-definition@2.2141.v5402e818a_779\">",
      "      <jobProperties>",
      "        <string>jenkins.model.BuildDiscarderProperty</string>",
      "      </jobProperties>",
      "      <triggers/>",
      "      <parameters/>",
      "      <options/>",
      "    </org.jenkinsci.plugins.pipeline.modeldefinition.actions.DeclarativeJobPropertyTrackerAction>",
      "  </actions>",
      "  <description>__APP_NAME__</description>",
      "  <keepDependencies>false</keepDependencies>",
      "  <properties>",
      "    <com.sonyericsson.rebuild.RebuildSettings plugin=\"rebuild@320.v5a_0933a_e7d61\">",
      "      <autoRebuild>false</autoRebuild>",
      "      <rebuildDisabled>false</rebuildDisabled>",
      "    </com.sonyericsson.rebuild.RebuildSettings>",
      "    <jenkins.model.BuildDiscarderProperty>",
      "      <strategy class=\"hudson.tasks.LogRotator\">",
      "        <daysToKeep>-1</daysToKeep>",
      "        <numToKeep>15</numToKeep>",
      "        <artifactDaysToKeep>-1</artifactDaysToKeep>",
      "        <artifactNumToKeep>-1</artifactNumToKeep>",
      "      </strategy>",
      "    </jenkins.model.BuildDiscarderProperty>",
      "  </properties>",
      "  <definition class=\"org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition\" plugin=\"workflow-cps@3697.vb_470e454c232\">",
      "    <scm class=\"hudson.plugins.git.GitSCM\" plugin=\"git@5.1.0\">",
      "      <configVersion>2</configVersion>",
      "      <userRemoteConfigs>",
      "        <hudson.plugins.git.UserRemoteConfig>",
      "          <name>origin</name>",
      "          <url>http://${GIT_SERVER}/${PID}-__APP_NAME__.git</url>",
      "          <credentialsId>${PID}-git</credentialsId>",
      "        </hudson.plugins.git.UserRemoteConfig>",
      "      </userRemoteConfigs>",
      "      <branches>",
      "        <hudson.plugins.git.BranchSpec>",
      "          <name>*/main</name>",
      "        </hudson.plugins.git.BranchSpec>",
      "      </branches>",
      "      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>",
      "      <submoduleCfg class=\"empty-list\"/>",
      "      <extensions/>",
      "    </scm>",
      "    <scriptPath>Jenkinsfile</scriptPath>",
      "    <lightweight>true</lightweight>",
      "  </definition>",
      "  <triggers/>",
      "  <disabled>false</disabled>",
      "</flow-definition>"
    ],
    "description": "C_JOB_PIPELINE_FROM_SCM",
    "hidden": true,
    "type": "system"
  },
