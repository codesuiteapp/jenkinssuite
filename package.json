    "menus": {
      "commandPalette": [
        {
          "command": "utocode.helloWorld",
          "when": "true"
        }
      ],
      "view/item/context": [
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*sparrow.*/",
          "command": "utocode.openLink#sparrow",
          "group": "inline@1"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_sqube.*/",
          "command": "utocode.openLink#sqube",
          "group": "inline@3"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_git.*/",
          "command": "utocode.openLink#git",
          "group": "inline@5"
        },
        {
          "when": "view == 'utocode.views.connection'",
          "command": "utocode.openLink#Home",
          "group": "inline@7"
        },
        {
          "when": "view == 'utocode.views.connection'",
          "command": "utocode.connectServer",
          "group": "1_jenkinssuite_actions@1"
        },
        {
          "when": "view == 'utocode.views.connection'",
          "command": "utocode.disconnectServer",
          "group": "1_jenkinssuite_actions@1"
        },
        {
          "when": "view == 'utocode.views.connection'",
          "command": "utocode.connectSSH",
          "group": "1_run@1"
        },
        {
          "when": "view == 'utocode.views.connection'",
          "command": "utocode.connectSSHExternal",
          "group": "1_run@1"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_admin.*/",
          "command": "utocode.createUser",
          "group": "2_configure@1"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_admin.*/",
          "command": "utocode.changeExecutor",
          "group": "2_configure@2"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_admin.*/",
          "command": "utocode.createCredential#SecretText",
          "group": "2_configure@2"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_admin.*/",
          "command": "utocode.createCredential#CredentialUser",
          "group": "2_configure@2"
        },
        {
          "when": "view == 'utocode.views.connection' && viewItem =~ /^.*_not.*/",
          "command": "utocode.setPrimaryServer",
          "group": "2_configure@1"
        },
        {
          "when": "view == 'utocode.views.views'",
          "command": "utocode.getConfigView",
          "group": "1_jenkinssuite_actions@2"
        },
        {
          "when": "view == 'utocode.views.views' && viewItem =~ /^.*_enabled.*/",
          "command": "utocode.renameView",
          "group": "1_jenkinssuite_actions@2"
        },
        {
          "when": "view == 'utocode.views.views'",
          "command": "utocode.openLinkView",
          "group": "inline"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.openLinkJob",
          "group": "inline"
        },
        {
          "when": "view == 'utocode.views.jobs' && viewItem =~ /^.*home.*/",
          "command": "utocode.openLink#appHome",
          "group": "inline"
        },
        {
          "when": "view == 'utocode.views.jobs' && viewItem =~ /^.*swagger.*/",
          "command": "utocode.openLink#swagger",
          "group": "inline"
        },
        {
          "when": "view == 'utocode.views.jobs' && viewItem =~ /^.*manage.*/",
          "command": "utocode.openLink#management",
          "group": "inline"
        },
        {
          "when": "viewItem == 'jobs'",
          "command": "utocode.buildJob",
          "group": "1_jenkinssuite_actions@1"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.getConfigJob",
          "group": "1_jenkinssuite_actions@3"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.openExternalBrowser",
          "group": "1_jenkinssuite_actions@5"
        },
        {
          "when": "viewItem == 'jobs' && viewItem != 'jobs_disabled'",
          "command": "utocode.addReservation",
          "group": "1_run@1"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.renameJob",
          "group": "2_configure@1"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.copyJob",
          "group": "2_configure@3"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.moveJob",
          "group": "2_configure@5"
        },
        {
          "when": "viewItem =~ /^jobs.*/",
          "command": "utocode.deleteJob",
          "group": "2_configure@7"
        },
        {
          "when": "viewItem == 'jobs_disabled'",
          "command": "utocode.enabledJob",
          "group": "3_manage@1"
        },
        {
          "when": "viewItem == 'jobs' && viewItem != 'jobs_disabled'",
          "command": "utocode.disabledJob",
          "group": "3_manage@1"
        },
        {
          "when": "viewItem == 'builds'",
          "command": "utocode.getJobLog",
          "group": "inline"
        },
        {
          "when": "viewItem == 'builds'",
          "command": "utocode.withJobLog",
          "group": "inline"
        },
        {
          "when": "viewItem == 'reservation'",
          "command": "utocode.cancelReservation",
          "group": "inline"
        },
        {
          "when": "viewItem == 'notify'",
          "command": "utocode.viewJobConsole",
          "group": "inline"
        },
        {
          "when": "viewItem == 'notify'",
          "command": "utocode.openLinkNotifyJob",
          "group": "inline"
        },
        {
          "when": "viewItem == 'snippet'",
          "command": "utocode.generateCode",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "utocode.connections.settings",
          "when": "view == utocode.views.connection",
          "group": "navigation@1"
        },
        {
          "command": "utocode.connections.refresh",
          "when": "view == utocode.views.connection",
          "group": "navigation@5"
        },
        {
          "command": "utocode.createView",
          "when": "view == utocode.views.views",
          "group": "navigation@1"
        },
        {
          "command": "utocode.views.refresh",
          "when": "view == utocode.views.views",
          "group": "navigation@5"
        },
        {
          "command": "utocode.generateJobCode",
          "when": "view == utocode.views.jobs",
          "group": "navigation@1"
        },
        {
          "command": "utocode.createFolder",
          "when": "view == utocode.views.jobs",
          "group": "navigation@2"
        },
        {
          "command": "utocode.jobs.refresh",
          "when": "view == utocode.views.jobs",
          "group": "navigation@5"
        },
        {
          "command": "utocode.builds.refresh",
          "when": "view == utocode.views.builds",
          "group": "navigation@5"
        },
        {
          "command": "utocode.reservation.info",
          "when": "view == utocode.views.reservation",
          "group": "navigation@1"
        },
        {
          "command": "utocode.reservation.sort",
          "when": "view == utocode.views.reservation",
          "group": "navigation@3"
        },
        {
          "command": "utocode.reservation.refresh",
          "when": "view == utocode.views.reservation",
          "group": "navigation@5"
        },
        {
          "command": "utocode.snippets.refresh",
          "when": "view == utocode.views.snippets",
          "group": "navigation@5"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Jenkins Suite",
      "properties": {
        "jenkinssuite.views.notify.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%views.notify.enabled.description%"
        },
        "jenkinssuite.views.reservation.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%views.reservation.enabled.description%"
        },
        "jenkinssuite.views.snippet.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%views.snippet.enabled.description%"
        },
        "jenkinssuite.server.primary": {
          "type": "string",
          "default": "",
          "description": "%server.primary.description%"
        },
        "jenkinssuite.plugin.jobdsl.enabled": {
          "type": "boolean",
          "default": "false",
          "description": "%plugin.jobdsl.enabled.description%"
        },
        "jenkinssuite.plugin.categorized.enabled": {
          "type": "boolean",
          "default": "false",
          "description": "%plugin.categorized.enabled.description%"
        },
        "jenkinssuite.snippet.custom-file": {
          "type": "string",
          "default": "",
          "description": "%snippet.custom-file.description%"
        },
        "jenkinssuite.snippet.create-view": {
          "type": "string",
          "default": "c_view_cateview",
          "description": "%snippet.create-view.description%"
        },
        "jenkinssuite.snippet.create-folder": {
          "type": "string",
          "default": "c_folder",
          "description": "%snippet.create-folder.description%"
        },
        "jenkinssuite.build.delay": {
          "type": "number",
          "default": 3,
          "description": "%build.delay.description%"
        },
        "jenkinssuite.limit.builds": {
          "type": "number",
          "default": "20",
          "enum": [
            "10",
            "20",
            "30",
            "50"
          ],
          "enumDescriptions": [
            "Max 10",
            "Max 20",
            "Max 30",
            "Max 50"
          ],
          "description": "%limit.builds.description%"
        },
        "jenkinssuite.limit.history": {
          "type": "number",
          "default": "15",
          "enum": [
            "15",
            "30",
            "50"
          ],
          "enumDescriptions": [
            "Max 15",
            "Max 30",
            "Max 50"
          ],
          "description": "%limit.history.description%"
        },
        "jenkinssuite.servers": {
          "type": "object",
          "description": "%property.servers.description%",
          "patternProperties": {
            ".+": {
              "type": "object",
              "required": [
                "url",
                "username",
                "token"
              ],
              "properties": {
                "url": {
                  "description": "URL to Jenkins host. Required",
                  "type": "string"
                },
                "description": {
                  "description": "Description of the Jenkins Server. Required",
                  "type": "string"
                },
                "username": {
                  "description": "username to Jenkins host. Required",
                  "type": "string"
                },
                "token": {
                  "description": "Token to Jenkins host. Required",
                  "type": "string"
                },
                "ssh.enabled": {
                  "description": "SSH Server Enabled",
                  "type": "boolean"
                },
                "ssh.address": {
                  "description": "SSH URL to SSH",
                  "type": "string"
                },
                "ssh.port": {
                  "description": "SSH Port number to SSH",
                  "type": "number"
                },
                "ssh.username": {
                  "description": "Username to SSH",
                  "type": "string"
                },
                "ssh.externalPath": {
                  "description": "external Program Path to SSH",
                  "type": "string"
                },
                "ssh.externalArg": {
                  "description": "external Program Argument to SSH",
                  "type": "string"
                },
                "ssh.extraArg": {
                  "description": "external Program Extra Argument to SSH",
                  "type": "string"
                },
                "wstalk.enabled": {
                  "description": "Wstalk Plugin Installed",
                  "type": "boolean"
                },
                "wstalk.url": {
                  "description": "Wstalk URL to Jenkins Wstalk host",
                  "type": "string"
                },
                "wstalk.description": {
                  "description": "Wstalk to Jenkins Wstalk description",
                  "type": "string"
                },
                "git": {
                  "description": "Git URL",
                  "type": "string"
                },
                "sonarqube": {
                  "description": "Sonarqube URL",
                  "type": "string"
                },
                "sparrow": {
                  "description": "Sparrow URL",
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "local": {
              "url": "http://localhost:8080/jenkins",
              "description": "Local Server",
              "username": "admin",
              "token": "",
              "ssh": {
                "enabled": false,
                "address": "192.168.0.1",
                "port": 22,
                "username": "root",
                "externalPath": "putty.exe",
                "externalArg": "-P"
              },
              "wstalk": {
                "enabled": false,
                "url": "ws://localhost:9090/jenkins",
                "description": "Local WsTalk Server"
              }
            },
            "test": {
              "url": "http://localhost:8080/jenkins",
              "description": "Test Server",
              "username": "admin",
              "token": "",
              "ssh": {
                "enabled": false,
                "address": "192.168.0.1",
                "port": 22,
                "username": "root",
                "externalPath": "putty.exe",
                "externalArg": "-P"
              },
              "wstalk": {
                "enabled": false,
                "url": "ws://localhost:9090/jenkins",
                "description": "Test WsTalk Server"
              }
            }
          }
        }
      }
    },
    "walkthroughs": [
      {
        "id": "utocode.jenkins-explorer",
        "title": "Jenkins Suite",
        "description": "%walkthroughs.description%",
        "steps": [
          {
            "id": "utocode.jenkins",
            "title": "%walkthroughs.step1.title%",
            "description": "%walkthroughs.step1.description%",
            "media": {
              "image": "images/guide/guide1.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:utocode.switchConnection"
            ]
          },
          {
            "id": "utocode.runSwitchConnection",
            "title": "%walkthroughs.step2.title%",
            "description": "%walkthroughs.step2.description%",
            "media": {
              "image": "images/guide/guide2.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:utocode.switchView"
            ]
          },
          {
            "id": "utocode.runJobView",
            "title": "%walkthroughs.step3.title%",
            "description": "%walkthroughs.step3.description%",
            "media": {
              "image": "images/guide/guide3.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:utocode.runJob"
            ]
          },
          {
            "id": "utocode.runGenerateJobCode",
            "title": "%walkthroughs.step4.title%",
            "description": "%walkthroughs.step4.description%",
            "media": {
              "image": "images/guide/guide5.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:utocode.generateJobCode"
            ]
          },
          {
            "id": "utocode.runValidateJenkins",
            "title": "%walkthroughs.step5.title%",
            "description": "%walkthroughs.step5.description%",
            "media": {
              "image": "images/guide/guide7.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:utocode.validateJenkins"
            ]
          }
        ]
      }
    ],
    "keybindings": [
      {
        "command": "workbench.view.extension.jenkins-explorer",
        "key": "alt+f8",
        "mac": "cmd+f8"
      },
      {
        "command": "utocode.withView",
        "key": "ctrl+k f3",
        "mac": "cmd+k f3"
      },
      {
        "command": "utocode.withJob",
        "key": "ctrl+k f4",
        "mac": "cmd+k f4"
      },
      {
        "command": "utocode.switchConnection",
        "key": "alt+1",
        "mac": "option+1"
      },
      {
        "command": "utocode.switchView",
        "key": "alt+2",
        "mac": "option+2"
      },
      {
        "command": "utocode.switchJob",
        "key": "alt+3",
        "mac": "option+3"
      },
      {
        "command": "utocode.runJob",
        "key": "alt+4",
        "mac": "option+4"
      },
      {
        "command": "utocode.runFolderJob",
        "key": "alt+6",
        "mac": "option+6"
      },
      {
        "command": "utocode.switchBuild",
        "key": "alt+5",
        "mac": "option+5"
      },
      {
        "command": "utocode.runAddReservation",
        "key": "alt+7",
        "mac": "option+7"
      },
      {
        "command": "utocode.validateJenkins",
        "key": "ctrl+alt+t",
        "mac": "cmd+option+t"
      },
      {
        "command": "utocode.showJobs",
        "title": "Show Jobs",
        "category": "jenkinssuite"
      },
      {
        "command": "utocode.showBuilds",
        "title": "Show Builds",
        "category": "jenkinssuite"
      },
      {
        "command": "utocode.showHistory",
        "title": "Show History",
        "category": "jenkinssuite"
      },
      {
        "command": "utocode.generateJobCode",
        "key": "ctrl+alt+insert",
        "mac": "cmd+option+f12",
        "category": "jenkinssuite"
      },
      {
        "command": "utocode.generateJobCodePick",
        "key": "ctrl+alt+shift+insert",
        "mac": "cmd+option+shift+f12",
        "category": "jenkinssuite"
      },
      {
        "command": "utocode.withJob",
        "key": "shift+alt+enter",
        "mac": "shift+option+enter",
        "when": "editorTextFocus",
        "category": "jenkinssuite"
      }
    ],
    "languages": [
      {
        "id": "jenkins",
        "extensions": [
          ".groovy",
          ".jenkins",
          "Jenkinsfile"
        ],
        "aliases": [
          "Jenkins"
        ],
        "configuration": "./languages/jenkins-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jenkins",
        "scopeName": "source.jenkins",
        "path": "./syntaxes/jenkins.tmGrammar.json"
      }
    ]
  },
  "scripts": {
    "minify": "node minify.js",
    "vscode:prepublish1": "npm run compile && npm run minify",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/l10n-dev": "0.0.26",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "formdata-node": "^5.0.1",
    "html-entities": "^2.3.6",
    "lodash": "^4.17.21",
    "ws": "^8.13.0",
    "xml-js": "^1.6.11"
  }
}
