{
  "name": "jenkinssuite",
  "displayName": "Jenkins Suite",
  "description": "Manage your Jenkins Job inside Visual Studio Code",
  "version": "0.1.3",
  "author": "utocode",
  "publisher": "utocode",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/utocode/jenkins-suite"
  },
  "license": "GNU General Public License v2.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Debuggers",
    "Extension Packs",
    "Linters",
    "Programming Languages",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "jenkins",
    "jenkinssuite",
    "shortcut",
    "jenkins dashboard",
    "vscode",
    "pipeline",
    "pipeline validation",
    "devops",
    "ci",
    "cd",
    "snippet",
    "monitoring",
    "management"
  ],
  "capabilities": {
    "virtualWorkspaces": true
  },
  "activationEvents": [],
  "extensionDependencies": [],
  "files": [
    "out/",
    "l10n/",
    "media/",
    "images/",
    "snippets/",
    "syntaxes/",
    "languages/",
    "resources/"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
